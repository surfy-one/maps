			"cities": {
				"minzoom": 8,
				"data": [ "name", "capital" ]
			},

			"countries": {
				"minzoom": 2,
				"data": [ "name" ]
			},

			"marine": {
				"minzoom": 2,
				"data": [ "name" ]
			}
		}
